<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <LinearGradientBrush x:Key="BlueGradientBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="{StaticResource BlueDeepBlack}" Offset="0.0"/>
        <GradientStop Color="{StaticResource BlueBlack}" Offset="0.5"/>
        <GradientStop Color="{StaticResource BlueDark}" Offset="1.0"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="WhiteGradientBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#40FFFFFF" Offset="0.0"/>
        <GradientStop Color="#30FFFFFF" Offset="0.5"/>
        <GradientStop Color="#10FFFFFF" Offset="1.0"/>
    </LinearGradientBrush>


</ResourceDictionary>